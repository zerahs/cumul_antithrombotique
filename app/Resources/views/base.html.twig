<!DOCTYPE html>
<html>
    <head>
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-121043523-1"></script>
        <script>
          window.dataLayer = window.dataLayer || [];
          function gtag(){dataLayer.push(arguments);}
          gtag('js', new Date());

          gtag('config', 'UA-121043523-1');
        </script>

        <meta charset="UTF-8" />
        <title>{% block title %}Combi-AT{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/app.css') }}">
        {% endblock %}
    </head>
    <body>
        {% block scripts %}
            <script src="{{ asset('build/app.js') }}"></script>
        {% endblock %}

        {% block header %}
            {{ include("header.html.twig", with_context = false) }}
        {% endblock %}

        <div class="container">
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
